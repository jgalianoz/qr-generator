{
  "name": "qr-generator",
  "displayName": "QR Generator",
  "description": "Extension to generate QR CODE",
  "version": "0.0.1",
  "publisher": "jgalianoz",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:qr-generator.generateQR"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qr-generator.generateQR",
        "title": "Generate QR"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "qr-generator.generateQR"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "qrcode-generator": "^1.4.4"
  }
}
